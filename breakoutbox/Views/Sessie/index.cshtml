@model IEnumerable<Sessie>

@{
    ViewData["Title"] = "Sessies";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    @foreach (var sessie in Model)
    {
        <tr>
            <td>
                <a asp-controller="Groep" asp-action="Index" asp-route-id="@sessie.Naam">@sessie.Naam</a>
                <ul>
                    <li></li>
                </ul>
            </td>
           <td>@sessie.Startdatum</td>
        </tr>
    }
</table>